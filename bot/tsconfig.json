{
	/* Visit https://aka.ms/tsconfig.json to read more about this file */
	"include": ["src/**/*.ts","src/**/*.d.ts"],
	"compilerOptions": {
		"target": "ES2021",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "./dest",
	}
}
